{
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/get-latest-cv.js": {
      "runtime": "nodejs18.x",
      "includeFiles": "Credentials/**"
    }
  },
  "headers": [
    {
      "source": "/Credentials/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600, must-revalidate" }
      ]
    }
  ]
}
